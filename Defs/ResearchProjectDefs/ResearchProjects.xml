		<requiredResearchFacilities>
			<li>MultiAnalyzer</li>
		</requiredResearchFacilities>
		<tab>MSE</tab>
		<researchViewX>8</researchViewX>
		<researchViewY>4.5</researchViewY>
	</ResearchProjectDef>

