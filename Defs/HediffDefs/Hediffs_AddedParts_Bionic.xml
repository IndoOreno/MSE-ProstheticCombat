<Defs>

	<HediffDef ParentName="MSE_AddedBodyPartBionic">
		<defName>MSEPC_PCBSD_AR</defName>
		<label>PCB SD-AR</label>
		<labelNoun>an PCB SD-AR</labelNoun>
		<comps>
			<li Class="OrenoPCF.HediffCompProperties_VerbGiverExtended">
				<toggleLabel>PCB SD-AR</toggleLabel>
				<toggleDescription>PCB SD-AR ({0})\n\nA weaponized bionic {1}. With its mounted assault rifle weapon, it has good range, decent power, and good accuracy bionic weapon.</toggleDescription>
				<toggleBackgroundPath>UI/Widgets/Prosthetic/DesButBGLG</toggleBackgroundPath>
				<toggleIconPath>Things/Item/Equipment/WeaponRanged/AssaultRifle</toggleIconPath>
				<toggleIconAngle>-45.0</toggleIconAngle>
				<verbs>
					<li>
						<label>assault rifle</label>
						<verbClass>Verb_Shoot</verbClass>
						<accuracyTouch>0.30</accuracyTouch>
						<accuracyShort>0.35</accuracyShort>
						<accuracyMedium>0.33</accuracyMedium>
						<accuracyLong>0.28</accuracyLong>
						<hasStandardCommand>true</hasStandardCommand>
						<defaultProjectile>Bullet_AssaultRifle</defaultProjectile>
						<warmupTime>1.85</warmupTime>
						<range>30.9</range>
						<ticksBetweenBurstShots>10</ticksBetweenBurstShots>
						<burstShotCount>3</burstShotCount>
						<soundCast>Shot_AssaultRifle</soundCast>
						<soundCastTail>GunTail_Medium</soundCastTail>
						<muzzleFlashScale>6</muzzleFlashScale>
					</li>
				</verbs>
				<verbsProperties>
					<li>
						<label>assault rifle</label>
						<description>PCB SD-AR - Assault rifle: A weapon that mounted to your pawn {0}. A general-purpose bionic gas-operated assault rifle for field or urban combat. It has good range, decent power, and good accuracy. It only has half stats over normal assault rifle weapon.</description>
						<uiIconPath>UI/Icons/Prosthetic/BulletSB</uiIconPath>
					</li>
				</verbsProperties>
			</li>
		</comps>
		<spawnThingOnRemoved>MSEPC_PCBSD_AR</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.25</partEfficiency>
		</addedPartProps>
	</HediffDef>

